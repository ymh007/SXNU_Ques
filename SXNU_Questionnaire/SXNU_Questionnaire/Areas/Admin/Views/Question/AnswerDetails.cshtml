@{
    Layout = "~/Areas/Admin/Views/Shared/_LayoutQues.cshtml";
    ViewBag.flgStyle = "q";
    ViewBag.Title = "问卷管理-问卷管理列表";
}
<script type="text/javascript">
    $(function () {
        var pageModel = new SXNU_ViewModel_AnswerDeail($);
        ko.applyBindings(pageModel, $("#ko_Question")[0]);
    });
</script> 
<div class="right p-w92" id="ko_Question">
    <div class="admin_name admin_zi">问卷管理 -- 问卷管理 -- <span>问卷管理列表</span></div>
    <div class="admin_frame min_h">
        <input type="hidden" value="@ViewBag.wjid" id="wjid" />
        <div class="m-b30 m-t15">
            <input type="submit" onclick="javascript: window.location.href = '/admin/Question/QuesList'" value="返回" class="a_red_min w-86 left m-r35" />
            <input type="button" value="导出问卷" class="a_red_min w-95 left m-r15"  data-bind="click:$root.Export_WJ"/> 
            <input type="button" value="导出回收数据" class="a_red_min w-125 left" data-bind="click:$root.Export_AnswerList" /> 
            <div class="clear"></div>
        </div>
        <table border="0" cellspacing="0" cellpadding="0" width="100%" class="volume_list">
            <tr>
                <td class="p-w20"><b>编号</b></td>
                <td class="p-w30"><b>姓名</b></td>
                <td class="p-w30"><b>答卷时间</b></td>
                <td class="p-w20"><b>操作</b></td>
            </tr>
        </table>
        <table id="mytable" border="0" cellspacing="0" cellpadding="0" width="100%" class="volume_list">
            <tbody data-bind="foreach:AnswerPerson_List">
                <tr>
                    <td class="p-w20" data-bind="text:Row"></td>
                    <td class="p-w30" data-bind="text:au_Name"></td>
                    <td class="p-w30" data-bind="text: (Math.floor(parseInt(au_Time.length==0?0:au_Time) / 60 ) +'分'+(parseInt(au_Time.length==0?0:au_Time) - (Math.floor(parseInt(au_Time.length==0?0:au_Time) / 60 ) * 60))+'秒')">30分40秒</td>
                    <td class="p-w20">
                        <span class="ico vol_ico5 m-r20"><a href="javascript:;" data-bind="click:$root.ViewAnswer">查看答题详情</a></span>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
    <div class="waiks">
        <ul class="paging">
            <li><a href="#" data-bind="visible:$root.am_CurrenPageIndex()!=0,click:$root.Provider">上一页</a></li>
            <li><a href="javascript:void(0)" data-bind="visible:$root.am_CurrenPageIndex()!=0,text:$root.am_CurrenPageIndex(),click:function(){$root.GotoPage($root.am_CurrenPageIndex()-1)} ">1</a></li>
            <li><a class="a_active" data-bind="text:$root.am_CurrenPageIndex()+1">2</a></li>
            <li><a href="javascript:void(0)" data-bind="visible:$root.am_CurrenPageIndex()!=($root.am_TotalPage()-1),text:$root.am_CurrenPageIndex()+2,click: function(){ $root.GotoPage($root.am_CurrenPageIndex()+1) }  ">3</a></li>
            <li><a href="#" data-bind="visible:$root.am_CurrenPageIndex()!=($root.am_TotalPage()-1),click:$root.NextPage">下一页</a></li>
        </ul>
        <div class="clear"></div>
    </div>
</div>
