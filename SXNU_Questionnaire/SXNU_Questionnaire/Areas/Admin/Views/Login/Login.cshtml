<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>陕师大-智能测评系统</title>
    <meta name="keywords" content="{$SEO['index_keyword']}">
    <meta name="description" content="{$SEO['index_description']}">
    
    @Scripts.Render("~/js/Common")
    @Scripts.Render("~/js/admin/Page/login")
    @Styles.Render("~/Content/admin/PageCss")
    <script type="text/javascript">
    $(function () {
        var pageModel = new SXNU_ViewModel_login($);
        ko.applyBindings(pageModel, $("#body")[0]);
    });
    </script>

</head>
<body class="bj"> 
    <div class="center admin_top"><img src="~/Content/images/admin/admin_logo.png" /></div>
    <div class="Sign_bj p-t100 p-l170">
        <form action="/Admin/Login/Login" method="post">
            <input type="text" name="user" class="Sign_in user w-310 p-l38" value="用户名" data-bind="value:Loginuser" />
            <input type="password" name="pwd" class="Sign_in pass w-310 p-l38 m-t15" value="密码" data-bind="value:Pwd" />
            <input type="text" name="code" class="Sign_in w-210 m-t15 p-l13 left" value="验证码" data-bind="value:V_Code" /><div class="left m-t16 m-l15"><img id="imgCode" data-bind="click:$root.ChangeCode" src="~/Admin/Login/GetCode?ID=1" /></div>
            <div class="clear"></div>
            <div class="sign_red m-t15 m-b15" id="ErrorMsg">@ViewData["ErrorMsg"]</div>
            <input class="w-350 sign_bus" type="submit" data-bind="click:Login" name="" value="登 录" />
        </form>
    </div>
    <div class="sign_bottom">@Html.Raw("@ 陕西师范大学          技术支持：西电易达")</div>

</body>
</html>
 